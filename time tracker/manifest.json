{
    "manifest_version": 2,
    "name": "Time Tracker Extension",
    "version": "1.0",
    "description": "Track time spent on external websites.",
    "permissions": ["tabs" ,"activeTab"],
    "browser_action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
      }
    }
  }
  